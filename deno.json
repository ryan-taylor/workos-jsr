{
  "name": "@ryantaylor/workos",
  "version": "7.50.0",
  "exports": {
    ".": "./mod.ts"
  },
  "imports": {
    "iron-session": "npm:iron-session@8.0.4",
    "jose": "npm:jose@6.0.11",
    "leb": "npm:leb@1.0.0",
    "pluralize": "npm:pluralize@8.0.0",
    
    "crypto": "jsr:@std/crypto@^1.0.0",
    "node:crypto": "jsr:@std/crypto@^1.0.0",
    "node:http": "jsr:@std/http@^1.0.0",
    "node:https": "jsr:@std/http@^1.0.0",
    "fs/promises": "jsr:@std/fs@^1.0.0",
    
    "chai": "npm:chai@5.2.0",
    "nock": "npm:nock@14.0.4",
    "supertest": "npm:supertest@7.1.0",
    
    "@std/assert": "jsr:@std/assert@^1.0.0",
    "@std/flags": "jsr:@std/flags@^1.0.0",
    "@std/path": "jsr:@std/path@^1.0.0",
    "@std/fs": "jsr:@std/fs@^1.0.0",
    "@std/http": "jsr:@std/http@^1.0.0",
    
    "$fresh/": "https://deno.land/x/fresh@1.6.5/",
    "preact": "https://esm.sh/preact@10.19.6",
    "preact/hooks": "https://esm.sh/preact@10.19.6/hooks",
    "preact/jsx-runtime": "https://esm.sh/preact@10.19.6/jsx-runtime",
    "preact/jsx-dev-runtime": "https://esm.sh/preact@10.19.6/jsx-dev-runtime",
    "preact/compat": "https://esm.sh/preact@10.19.6/compat",
    "react": "https://esm.sh/react@18.2.0",
    "react-dom": "https://esm.sh/react-dom@18.2.0"
  },
  "tasks": {
    "test": "deno test --allow-all",
    "test:watch": "deno test --allow-all --watch",
    "test:coverage": "deno test --allow-all --coverage=cov",
    "coverage:report": "deno coverage cov --lcov --output=cov/lcov.info",
    "coverage:html": "deno coverage cov --html --output=cov/html",
    "test:deno": "deno test --allow-all tests_deno/",
    "test:deno:watch": "deno test --allow-all --watch tests_deno/",
    "test:deno:coverage": "deno test --allow-all --coverage=cov tests_deno/",
    "test:ci": "deno test --allow-all --coverage=cov && deno coverage cov --lcov --output=cov/lcov.info",
    "bundle": "deno run -A https://deno.land/x/fresh@1.6.5/dev.ts build",
    "bundle:sdk": "deno run -A ./scripts/bundle.ts",
    "deploy": "deno run -A ./scripts/bundle.ts && deno deploy"
  },
  "test": {
    "include": [
      "src/**/*.{test,spec}.{ts,js}",
      "tests/**/*.test.{ts,js}",
      "main_test.ts"
    ],
    "exclude": [
      "**/node_modules/**"
    ]
  },
  "compilerOptions": {
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "strictBindCallApply": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "emitDecoratorMetadata": true,
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "lib": ["dom", "deno.ns", "deno.window"],
    "types": ["vitest"]
  },
  "fmt": {
    "indentWidth": 2,
    "lineWidth": 150,
    "semiColons": true,
    "singleQuote": true,
    "proseWrap": "preserve",
    "include": ["src/", "tests/", "tests_deno/", "scripts/", "*.ts", "examples/"]
  },
  "lint": {
    "include": ["src/", "tests/", "tests_deno/", "scripts/", "*.ts", "examples/"],
    "exclude": ["**/node_modules/**", "lib/**", "cov/**", "coverage/**", "dist/**", ".sailplane/**"],
    "rules": {
      "exclude": [
        "no-explicit-any",
        "no-unused-vars",
        "ban-ts-comment"
      ]
    }
  },
  "publish": {
    "exclude": [
      ".env*",
      ".git*",
      "coverage/",
      "tests/",
      "examples/",
      "_reference/",
      "scripts/",
      "cov/",
      "lib/",
      "node_modules/"
    ]
  }
}
