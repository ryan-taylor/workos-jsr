{
  "name": "@ryantaylor/workos",
  "version": "1.0.0",
  "exports": "./mod.ts",
  "description": "Pure Deno implementation of the WorkOS SDK with support for Fresh 1.x and 2.x",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryan-taylor/workos-jsr"
  },
  "homepage": "https://github.com/ryan-taylor/workos-jsr",
  "documentation": "https://workos.com/docs/reference/client-libraries",
  "authors": [
    {
      "name": "Ryan Taylor",
      "url": "https://twitter.com/ryantaylor"
    }
  ],
  "license": "MIT",
  "files": [
    "mod.ts",
    "packages/workos_sdk/mod.ts",
    "packages/workos_sdk/src/**/*.ts"
  ],
  "exclude": [
    "**/*.spec.ts",
    "**/*-test.ts",
    "**/*-live-test.spec.ts",
