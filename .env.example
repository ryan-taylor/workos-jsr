# API Key for WorkOS
WORKOS_API_KEY=sk_test_YOUR_KEY_HERE

# Client ID for WorkOS
WORKOS_CLIENT_ID=client_YOUR_CLIENT_ID_HERE

# Cookie password for session encryption
WORKOS_COOKIE_PASSWORD=use-a-strong-password-in-production

# Optional: API hostname (defaults to api.workos.com)
# WORKOS_API_HOSTNAME=api.workos.com

# Optional: Port for API requests
# WORKOS_PORT=443

# Optional: Use HTTPS (defaults to true)
# WORKOS_HTTPS=true

# Session secret for Fresh framework
SESSION_SECRET=use-a-strong-password-in-production